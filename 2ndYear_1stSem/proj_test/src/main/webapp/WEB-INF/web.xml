<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
         
         <servlet>
         	<servlet-name>AddPrescriptionServlet</servlet-name>
         	<servlet-class>servlet.AddPrescriptionServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>AddPrescriptionServlet</servlet-name>
         	<url-pattern>/AddPrescriptionServlet</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>Appointments</servlet-name>
         	<servlet-class>servlet.Appointments</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>Appointments</servlet-name>
         	<url-pattern>/Appointments</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>PatientRecordServlet</servlet-name>
         	<servlet-class>servlet.PatientRecordServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>PatientRecordServlet</servlet-name>
         	<url-pattern>/PatientRecordServlet</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>UpdateAppointmentServlet</servlet-name>
         	<servlet-class>servlet.UpdateAppointmentServlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>UpdateAppointmentServlet</servlet-name>
         	<url-pattern>/UpdateAppointmentServlet</url-pattern>
         </servlet-mapping>
         
         
        
         
</web-app>